<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TabPane fx:id="_tabPane_forms" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="_tab_cnf" text="CNF" />
          <Tab fx:id="_tab_dnf" text="DNF" />
        </tabs>
      </TabPane>
      <HBox prefWidth="150.0">
         <children>
            <Button fx:id="_button_yes" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Accept" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_button_no" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Reject" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
