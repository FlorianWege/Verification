<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.fxmisc.richtext.CodeArea?>

<VBox prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.151" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TabPane side="RIGHT" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="raw">
               <content>
                  <VBox>
                     <children>
                        <CodeArea fx:id="_textArea_loop_raw" />
                        <CodeArea fx:id="_textArea_loop_postCond" maxHeight="25.0" />
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab text="reduced">
               <content>
                  <VBox>
                     <children>
                        <CodeArea fx:id="_textArea_loop_reduced" />
                     </children>
                  </VBox>
               </content>
            </Tab>
         </tabs>
      </TabPane>
      <HBox>
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="loop cond" HBox.hgrow="ALWAYS" />
            <TextField fx:id="_textField_loop_loopCond_raw" promptText="&lt;loop cond&gt;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="119.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="loop cond (reduced)" HBox.hgrow="ALWAYS" />
            <TextField fx:id="_textField_loop_loopCond_reduced" promptText="&lt;loop cond reduced&gt;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="144.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="loop cond (CNF)" HBox.hgrow="ALWAYS" />
            <TextField fx:id="_textField_loop_loopCond_CNF" promptText="&lt;loop cond CNF&gt;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="119.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="post cond" HBox.hgrow="ALWAYS" />
            <TextField fx:id="_textField_loop_postCond_raw" promptText="&lt;post cond&gt;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="194.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="post cond (reduced)" HBox.hgrow="ALWAYS" />
            <TextField fx:id="_textField_loop_postCond_reduced" promptText="&lt;post cond reduced&gt;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="219.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="post cond (CNF)" HBox.hgrow="ALWAYS" />
            <TextField fx:id="_textField_loop_postCond_CNF" promptText="&lt;post cond CNF&gt;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
